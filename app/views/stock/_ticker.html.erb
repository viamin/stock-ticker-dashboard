<div class="ticker-wrapper" data-controller="konami" data-turbo-permanent>
  <div class="hwrap"><div class="hmove">
    <%- stocks.each do |stock| %>
      <div class="hitem"><%= link_to "#{stock.ticker} #{stock.latest_price} #{stock.price_trend_icon}", stock_path(stock.ticker), data: { turbo_frame: "stock-data" } %></div>
    <% end %>
  </div></div>
</div>
